{
  "name": "quickgui",
  "version": "0.1.0",
  "description": "A starting point to create ES6 module for browser",
  "repository": "jonathanlurie/quickgui",
  "moduleName": "quickgui",
  "entry": "src/main.js",
  "main": "dist/quickgui.js",
  "min": "dist/quickgui.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "serve . & rollup -w -c rollup.config.dev.js",
    "doc": "documentation build src/main.js -o ./doc/ -f html; documentation build src/** -f md > documentation.md",
    "lintfix": "standard --fix src/**/*.js",
    "lint": "standard src/**/*.js"
  },
  "author": "Jonathan Lurie",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "documentation": "^6.1.0",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "serve": "^6.3.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "dat.gui": "^0.7.1",
    "quickvoxelcore": "^0.1.4"
  }
}
